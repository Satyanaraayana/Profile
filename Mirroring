3M	     		16.00 MB

@BJC3MCSQL01
-------------
backup database 3M to disk ='\\BJC3MCSQL01DR\TempBackup2\3M.bak' with differential,stats=5
backup log 3M to disk ='\\BJC3MCSQL01DR\TempBackup2\3M.trn' with stats=5

backup database MMM360NormsDB to disk ='\\BJC3MCSQL01DR\TempBackup2\MMM360NormsDB.bak' with differential,stats=5
backup log MMM360NormsDB to disk ='\\BJC3MCSQL01DR\TempBackup2\MMM360NormsDB.trn' with stats=5

backup database QualityIndicators to disk ='\\BJC3MCSQL01DR\TempBackup2\QualityIndicators.bak' with differential,stats=5
backup log QualityIndicators to disk ='\\BJC3MCSQL01DR\TempBackup2\QualityIndicators.trn' with stats=5

backup database ConnectionHub to disk ='\\BJC3MCSQL01DR\TempBackup2\ConnectionHub.bak' with differential,stats=5
